# Environment Configuration
# Set to 'development' or 'production' (optional, defaults to __DEV__)
EXPO_PUBLIC_ENV=development

# Firebase Development Configuration
# Get these from Firebase Console > Project Settings > General > Your apps
# Use your DEVELOPMENT Firebase project credentials here
EXPO_PUBLIC_FIREBASE_DEV_API_KEY=your_dev_api_key_here
EXPO_PUBLIC_FIREBASE_DEV_AUTH_DOMAIN=your_dev_project_id.firebaseapp.com
EXPO_PUBLIC_FIREBASE_DEV_PROJECT_ID=your_dev_project_id
EXPO_PUBLIC_FIREBASE_DEV_STORAGE_BUCKET=your_dev_project_id.appspot.com
EXPO_PUBLIC_FIREBASE_DEV_MESSAGING_SENDER_ID=your_dev_messaging_sender_id
EXPO_PUBLIC_FIREBASE_DEV_APP_ID=your_dev_app_id

# Firebase Production Configuration
# Get these from Firebase Console > Project Settings > General > Your apps
# Use your PRODUCTION Firebase project credentials here
EXPO_PUBLIC_FIREBASE_PROD_API_KEY=your_prod_api_key_here
EXPO_PUBLIC_FIREBASE_PROD_AUTH_DOMAIN=your_prod_project_id.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROD_PROJECT_ID=your_prod_project_id
EXPO_PUBLIC_FIREBASE_PROD_STORAGE_BUCKET=your_prod_project_id.appspot.com
EXPO_PUBLIC_FIREBASE_PROD_MESSAGING_SENDER_ID=your_prod_messaging_sender_id
EXPO_PUBLIC_FIREBASE_PROD_APP_ID=your_prod_app_id
